  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "autoprefixer": "9.8.6",
    "babel-jest": "26.5.2",
    "babel-loader": "8.1.0",
    "css-loader": "4.2.2",
    "cssnano": "4.1.10",
    "editorconfig": "0.15.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.7.0",
    "eslint-config-htmlacademy": "0.5.2",
